<div id="content" class="container_16">
  <h1><%= @page_title = "New Project" %></h1>

  <% form_for(@project) do |form| %>
    <%= form.error_messages %>

    <%= render :partial=>'form', :object=>form%>

    <p>
      <%= form.submit 'Create' %> <%= link_to 'Cancel', root_path %>
    </p>
  <% end %>

</div>